        .community-panel-main-div {
          border: 0;
          border-radius: 0;
          margin-top: 0px;
        }

        .allSidesSoft {
          box-shadow: 0 3px 10px 0 rgba(115,143,147,.3);
        }

        @media (min-width: 768px) {
          .community-panel-main-div {
            margin-top: 50px;
          }
        }

